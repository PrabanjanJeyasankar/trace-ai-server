NODE_API_URL=http://localhost:8000/api/v1
EVAL_API_KEY=your-secure-eval-api-key-here

# LLM Provider Selection for Evaluation
# Options: "ollama" or "openai"
EVAL_LLM_PROVIDER=ollama

# Ollama Configuration (used when EVAL_LLM_PROVIDER=ollama)
OLLAMA_BASE_URL=http://127.0.0.1:11434
OLLAMA_MODEL=llama3.1:8b

# OpenAI Configuration (used when EVAL_LLM_PROVIDER=openai)
OPENAI_API_KEY=your-openai-api-key-here
OPENAI_MODEL=gpt-4o-mini
OPENAI_TEMPERATURE=0
